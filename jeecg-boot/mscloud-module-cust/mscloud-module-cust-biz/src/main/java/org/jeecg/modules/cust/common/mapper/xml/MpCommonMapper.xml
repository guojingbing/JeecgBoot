<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.jeecg.modules.cust.common.mapper.MpCommonMapper">
    <select id="selectDictItemsByDictCode" resultType="java.util.HashMap">
        SELECT
        a.*
        FROM
        sys_dict_item a LEFT JOIN sys_dict b ON a.dict_id=b.id
        WHERE
        1 = 1 AND ifnull(a.status,0)=1
        <if test="dictCode!=null">
            AND b.dict_code=#{dictCode}
        </if>
        ORDER BY
        sort_order ASC
    </select>
</mapper>